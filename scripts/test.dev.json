{
  "verbose": true,
  "rootDir": "../",
  "testURL": "http://localhost",
  "coverageDirectory": "test/coverage/",
  "testMatch": [
    "<rootDir>/test/*.test.js"
  ],
  "coverageReporters": [
    "html"
  ],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.js",
    "!**/node_modules/**"
  ]
}
